    <script type="text/javascript" language="javascript">
        function openpopup(popurl, name, param) {
            var winpops = window.open(popurl, name, param)
        }

        function check_login() {

            var ctl = "txtEmailID";
            var ctlpwd = "txtPassword";
            if (document.getElementById(ctl).value == '' && document.getElementById(ctlpwd).value == '') {
                alert('Emailid and Password should not be empty');
                document.getElementById(ctl).focus();

                return false;
            }
            else if (document.getElementById(ctl).value == '') {
                alert('Emailid  should not be empty');
                document.getElementById(ctl).focus();

                return false;
            }
            else if (document.getElementById(ctlpwd).value == '') {
                alert('Password  should not be empty');
                document.getElementById(ctlpwd).focus();

                return false;
            }

            else if (isEmail(document.getElementById(ctl).value) == false) {
                alert('The entered email address is invalid');
                document.getElementById(ctl).focus();

                return false;
            }

            // __doPostBack(ctlimgBtn, 'e');        		  
            //__doPostBack('imgBtnSubmit','My Argument');     
            return true;
        }

        function isEmail(string) {
            if (string.search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) != -1)
                return true;
            else
                return false;
        }


        function LetEnterKey() {
            if (window.event.keyCode == 13) {
                return check_login();

            }
        }
    </script>
